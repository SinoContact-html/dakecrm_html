.soft_top {height:106px; background:#4b515a url(../images/bg_soft_top.gif) left top repeat; position:relative; min-width:1230px;}
.soft_top .top_line_a {height:40px; padding-top:10px; position:relative; z-index:99; padding-left:100px;}
.soft_top .top_line_a .logoBox {width:76px; height:76px; background:url(../images/logo.png) no-repeat; position:absolute; left:10px; top:10px;}
.soft_top .top_line_a .logoBox a {display:block; width:76px; height:76px;}
.soft_top .top_line_b {height:56px; position:relative; z-index:98; padding-left:100px;}

.soft_top a.infoShowBox,
.soft_top span.infoShowBox {
	background:#1b212e;	border-radius:4px;	border:1px solid #151618; cursor:pointer; color:#dde7e9 !important;	text-decoration: none;
	display:inline-block; -webkit-box-shadow: inset 1px 1px 0 0 #293244, inset -1px -1px 0 0 #191d29;
	box-shadow: inset 1px 1px 0 0 #293244, inset -1px -1px 0 0 #191d29;
	line-height:14px; height:21px; font-weight:bold; min-width:20px;
	text-align:left; text-overflow:ellipsis; overflow:hidden; padding:6px 11px 0 11px; position:relative;
	vertical-align:top; white-space:nowrap; margin-right:4px;
}
.soft_top a.infoShowBox:hover,
.soft_top span.infoShowBox:hover{
	background-image:-webkit-linear-gradient(top, #414856, #323741);
	background-image:-moz-linear-gradient(top, #414856, #323741);
	background-image:-ms-linear-gradient(top, #414856, #323741);
	background-image:-o-linear-gradient(top, #414856, #323741);
	background-image: linear-gradient(top, #414856, #323741);
	-webkit-box-shadow: inset 1px 1px 0 0 rgba(255,255,255, 0.1), inset -1px -1px 0 0 rgba(255,255,255, 0.03);
