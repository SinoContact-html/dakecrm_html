@charset "utf-8";
/* CSS Document */

* {-webkit-text-size-adjust:none;}
html{-webkit-text-size-adjust:none;}
html, body, div, p, ul, li, dl, dt, dd, h1, h2, h3, h4, h5, h6, form, input, select, button, textarea, iframe, table, th, td { margin: 0; padding: 0; }
img {border: 0 none; vertical-align: top; }
ul, li {list-style-type: none; }
h1, h2, h3, h4, h5, h6 {font-size: 12px; }
body, input, select, button, textarea {font-size: 12px; font-family: Arial, "Microsoft Yahei", "微软雅黑"; }
button {cursor: pointer; }
i, em, cite { font-style: normal; }
body {background:#e5eef0 url(../images/bg_veins1.gif); color: #363636; line-height: 1.2; }
a, a:link {color:#222; text-decoration: none; }
a:visited {}
a:active, a:hover { text-decoration: underline; }
a:focus {outline: none;}

textarea {resize: none; border-radius: 4px; border:1px solid #a3a5a5}
input[type="text"] {border:1px solid #a3a5a5; height:29px; line-height:29px; -moz-box-shadow:0px 3px 3px #d8dedf inset; -webkit-box-shadow:0px 3px 3px #d8dedf inset; box-shadow:0px 3px 3px #d8dedf inset; padding-left:5px; border-radius:4px;}
input[type="submit"] {padding:0 10px; height:31px; line-height:31px; border-radius:4px; background:url(../images/bg_soft_right_top.gif) 0 -100px repeat-x; border:1px solid #8d9a9e; color:#3f4b54; font-size:13px; text-shadow:0 1px 0 rgba(255,255,255,0.5); cursor:pointer;}
input[type="submit"]:hover {background-position:0 -140px;}
select {background: #fff; border-radius:4px; border:1px solid #a3a5a5;-webkit-box-shadow:0 1px 0 0 rgba(255,255,255,0.3), inset 0 2px 2px -1px rgba(180,188,191,0.7);	box-shadow:0 1px 0 0 rgba(255,255,255,0.3), inset 0 2px 2px -1px rgba(180,188,191,0.7);	height:31px; margin:0; vertical-align:middle; padding:5px;}

a.onlymenu {display:block; float:left; padding:0 10px; height:29px; line-height:29px; border-radius:4px 4px 4px 4px; background:url(../images/bg_soft_right_top.gif) 0 -100px repeat-x; border:1px solid #8d9a9e; color:#3f4b54; font-size:13px; text-shadow:0 1px 0 rgba(255,255,255,0.5);}
 
.clearfix:after {content: ".";display: block;height: 0;clear: both;visibility: hidden;}
.clearfix {display: inline-block;}
/* Hides from IE-mac */
* html .clearfix { height: 1%;}
.clearfix {display: block;}
/* End hide from IE-mac */
.clear {diplay: block!important; float: none!important; clear: both; overflow: hidden; width: auto!important; height: 0!important; margin: 0 auto!important; padding: 0!important; font-size: 0; line-height: 0; }
.fl, .fr { display: inline; float: left; }
.fr { float: right; }
.pl12 {padding-left:12px;}
.mb10 {margin-bottom:10px !important;}
.mb20 {margin-bottom:20px !important;}
.mb30 {margin-bottom:30px !important;}
.mr30 {margin-right:30px !important;}
.mr20 {margin-right:20px !important;}
.mr10 {margin-right:10px !important;}
.fontsize14 {font-size:14px;}

span.input_file {-webkit-border-radius: 3px;-moz-border-radius: 3px;border:none;border-radius: 3px;background-color: #eaf1f3!important;-webkit-box-shadow: 0 0 1px rgba(0, 0, 0, 0.3), 0 1px 1px rgba(0, 0, 0, 0.3), 0 1px #FFFFFF inset, 0 0 1px rgba(255, 255, 255, 0.5) inset;box-shadow: 0 0 1px rgba(0, 0, 0, 0.3), 0 1px 1px rgba(0, 0, 0, 0.3), 0 1px #FFFFFF inset, 0 0 1px rgba(255, 255, 255, 0.5) inset;background-image: -webkit-linear-gradient(bottom, #d7e3e7, #fff);background-image: -moz-linear-gradient(bottom, #d7e3e7, #fff);background-image: -ms-linear-gradient(bottom, #d7e3e7, #fff);background-image: -o-linear-gradient(bottom, #d7e3e7, #fff);
	background-image: linear-gradient(bottom, #d7e3e7, #fff);color:#3f4b54;cursor:pointer;display:inline-block;font-weight:bold;font-size:13px;height: 18px;text-shadow:0 1px #fff;text-decoration:none;vertical-align:top;padding:6px 15px 5px 12px;position:relative;overflow: hidden;white-space: nowrap;}
span.input_file * {vertical-align:middle;}
span.input_file em {display:inline-block; *zoom:1; *display:inline; width:10px; height:18px; background:url(../images/icon.gif) 0 -1049px no-repeat; margin-right:5px;cursor:pointer;}
span.input_file span {line-height:18px;cursor:pointer;}
span.input_file input[type="file"] {right:0; top:0; opacity:0; -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=0)"; position:absolute; cursor:pointer;}
span.input_file:hover {background:#f5f7f8;}

input[type="radio"] {cursor:pointer;}
strong.bigtitle {font-size:24px; font-weight:normal;}

