.popbox_bg {position:absolute; z-index:999; width:100%; height:100%; top:0; background:#000; background:none\9; filter:alpha(opacity=70); -moz-opacity:0.7; -khtml-opacity: 0.7; opacity: 0.7; background:url(../images/bg_pop.png)\9; _background:url(../images/bg_pop.gif); display:none;}
.popbox_bg_remind {position:absolute; z-index:1001; width:100%; height:100%; top:0; background:#000; background:none\9; filter:alpha(opacity=70); -moz-opacity:0.7; -khtml-opacity: 0.7; opacity: 0.7; background:url(../images/bg_pop.png)\9; _background:url(../images/bg_pop.gif); display:none;}

.popbox_core {border-radius: 4px; background-color: #fff; -webkit-box-shadow: 0 18px 37px 6px rgba(16,39,53,.69); box-shadow: 0 18px 37px 6px rgba(16,39,53,.69);position: absolute;	overflow: hidden; width:750px; z-index:1000; overflow:hidden; display:none;}
.popbox_remind {width:650px; z-index:1002;}
.popbox_minisize {width:350px;}
.popbox_core .popbox_head {background-color: #e2ebee; border-bottom:1px solid#a9a9a9; -webkit-box-shadow: 0 1px 0 0 #e5e5e5, 0 -1px 0 0 #ecf1f3 inset, inset 0 1px 0 0 #fff, inset 0 0 0 1px #e9f0f2; box-shadow: 0 1px 0 0 #e5e5e5, 0 -1px 0 0 #ecf1f3 inset, inset 0 1px 0 0 #fff, inset 0 0 0 1px #e9f0f2;	border-radius:4px 4px 0 0;	height: 52px; line-height:49px;}
